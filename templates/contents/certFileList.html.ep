  <%# Main content %>
 
  <%# Modal %>
  <div class="modal fade" id="certDelModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Confirm</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          Do you really want to delete this file?
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-danger">Delete Anyway</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
  <%# <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#certDelModal">  %>
  <%# END Modal %>


    <div class="card">
      <div class="card-header">
      </div>
      <%# /.card-header %>
      <div class="card-body">
        <table id="tbcertfiles" class="table table-bordered table-striped table-hover">
          <thead>
          <tr>
            <th>FileName</th>
            <th>CreatedDate</th>
            <th>ExpireDate</th>
            <th>Action</th>
          </tr>
          </thead>

          <tfoot>
            <th>FileName</th>
            <th>CreatedDate</th>
            <th>ExpireDate</th>
            <th>Action</th>
          </tfoot>
        </table>
      </div>
      <%# /.card-body %>
    </div>

  </div>
  <%# /.content-wrapper %>
